Algoritmo while_cicle
	Definir  inicio, limite, incremento Como Entero;
	Escribir "donde inicia la impresion";
	leer inicio;
	Escribir  "limite";
	Leer  limite;
	Escribir "incremento";
	Leer incremento
	
	Mientras inicio <= limite Hacer
		Escribir " ",inicio," ";
		inicio <- inicio + incremento;
	Fin Mientras
	
	
FinAlgoritmo
